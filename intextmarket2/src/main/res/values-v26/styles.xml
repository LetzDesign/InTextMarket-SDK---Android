<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="CategoryButton">
        <item name="android:background">@drawable/imemojishapebk</item>
        <item name="android:backgroundTint">@color/icons</item>
        <item name="android:padding">8dp</item>
        <item name="android:autofilledHighlight">@color/colorAccent</item>
        <item name="android:elevation">3dp</item>
    </style>

    <style name="IMarketSearchFragmentButton">
        <item name="android:background">@color/icons</item>
        <item name="android:textSize">8sp</item>
        <item name="android:drawableTop">@drawable/ic_save_all_24_itw</item>
        <item name="android:text">@string/markets_list_fragment_save</item>
        <item name="android:autofilledHighlight" tools:targetApi="o">@color/colorPrimary</item>
    </style>

    <style name="IMarketShareFragmentButton">
        <item name="android:background">@color/icons</item>
        <item name="android:textSize">8sp</item>
        <item name="android:drawableTop">@drawable/ic_share_itw</item>
        <item name="android:text">@string/markets_list_fragment_share</item>
        <item name="android:autofilledHighlight" tools:targetApi="o">@color/colorPrimary</item>
    </style>

    <style name="IWebViewSlide">
        <item name="android:windowEnterAnimation">@anim/iweb_view_slideup_anim</item>
        <item name="android:windowExitAnimation">@anim/iweb_view_slideup_anim</item>
    </style>
</resources>